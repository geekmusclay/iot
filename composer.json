{
    "name": "geekmusclay/iot",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Geekmusclay\\IOT\\": "src/",
            "Geekmusclay\\IOT\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "geekmusclay",
            "email": "contact@geekmusclay.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8",
        "laminas/laminas-coding-standard": "^2.4",
        "symfony/var-dumper": "^5.4"
    },
    "scripts": {
        "analyse": "vendor\\bin\\phpstan analyse -c phpstan.neon",
        "tests": "vendor\\bin\\phpunit tests",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
